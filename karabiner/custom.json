{
  "title": "@_ddrscott_ custom settings",
  "rules": [{
    "description": "fn-(Y)ank fn-(P)ut",
    "manipulators": [
      {
        "from": {
          "key_code": "p",
          "modifiers": {
            "mandatory": [
              "fn"
            ],
            "optional": [
              "any"
            ]
          }
        },
        "to": [
          {
            "key_code": "insert",
            "modifiers": [
              "right_shift"
            ]
          }
        ],
        "type": "basic"
      },
      {
        "from": {
          "key_code": "y",
          "modifiers": {
            "mandatory": [
              "fn"
            ],
            "optional": [
              "any"
            ]
          }
        },
        "to": [
          {
            "key_code": "insert",
            "modifiers": [
              "right_control"
            ]
          }
        ],
        "type": "basic"
      }
    ]
  },
  {
    "description": "[Mac Keyboard with Citrix] Swap left option/command with special Alt-Tab handler",
    "manipulators": [
      {
        "conditions": [
          {
            "bundle_identifiers": [
              "^com\\.citrix.*"
            ],
            "type": "frontmost_application_if"
          },
          {
            "identifiers": [
              {
                "product_id": 2071,
                "vendor_id": 1118
              }
            ],
            "type": "device_unless"
          }
        ],
        "from": {
          "modifiers": {
            "optional": [
              "any"
              ]
          },
          "simultaneous": [
            {
              "key_code": "left_command"
            },
            {
              "key_code": "tab"
            }
          ],
          "simultaneous_options": {
            "key_down_order": "strict",
            "key_up_when": "all"
          }
        },
        "to": [
          {
            "key_code": "tab",
            "modifiers": [
              "right_option"
              ]
          },
          {
            "key_code": "right_option"
          }
        ],
        "type": "basic"
      },
